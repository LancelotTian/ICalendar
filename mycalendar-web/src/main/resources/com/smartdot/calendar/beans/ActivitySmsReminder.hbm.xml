<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.smartdot.calendar.beans.ActivitySmsReminder" table="smsreminder">
		<id name="id" column="ID" type="java.lang.String" length="32">
			<generator class="uuid"></generator>
		</id>
		<property name="activityId" column="ACTIVITYID" type="java.lang.String" length="32"></property>
		<property name="remindTime" column="REMINDTIME" type="java.lang.String" length="16"></property>
		<property name="searchDate" column="SDATE" type="java.util.Date"></property>
	</class>
</hibernate-mapping>
