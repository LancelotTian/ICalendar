<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="net.tl.icalendar.beans.CalendarTask" table="calendartask">
    <id column="ID" length="32" name="id" type="java.lang.String">
      <generator class="uuid"/>
    </id>
    <property column="TASKID" length="16" name="taskId" type="java.lang.String"/>
    <property column="TYPE" name="taskType" type="java.lang.Integer"/>
    <property column="DESC" length="1024" name="taskDesc" type="java.lang.String"/>
    <property column="SDATE" name="searchDate" type="java.util.Date"/>
  </class>
</hibernate-mapping>
